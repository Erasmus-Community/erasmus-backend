generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Organisation {
  country     String?
  description String?
  id          Int     @default(autoincrement()) @id
  name        String?
}

